{
  "name": "snow-music",
  "version": "1.1.0",
  "description": "An electron music app",
  "main": "src/main/index.ts",
  "scripts": {
    "dev:test": "cross-env NODE_ENV=development webpack --config webpack/webpack.main.prod.ts",
    "dev:main": "cross-env NODE_ENV=development webpack --config webpack/webpack.main.dev.ts && electron ./dist-webpack/main/main.js",
    "dev:renderer": "webpack-dev-server --hot --env.development --config webpack/webpack.renderer.dev.ts",

    "build:main": "webpack --config webpack/webpack.main.prod.ts",
    "build:renderer": "webpack --env.production --config webpack/webpack.renderer.prod.ts",
    "build": "npm run webpack-renderer-build && npm run electron-pack",

    "electron-pack": "npm run webpack-renderer-build && electron-builder --config electron-builder.json",
    "electron-pack-dir": "electron-builder --dir --config electron-builder.json",

    "publish": "npm run webpack-renderer-build && electron-builder --config electron-builder.json -p always",

    "type-check": "tsc"
  },
  "keywords": [],
  "author": "luohuidong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luohuidong/electron-music.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/classnames": "^2.2.10",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.12",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "eslint": "^7.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "prettier": "2.0.5",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/webpack-merge": "^4.1.5",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "electron-log": "^4.1.2",
    "electron-updater": "^4.3.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
