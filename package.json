{
  "name": "snow-music",
  "version": "1.2.0",
  "description": "An electron music app",
  "private": true,
  "main": "dist/dist-webpack/main/index.js",
  "scripts": {
    "dev:renderer": "webpack serve --hot --env development --config webpack/webpack.renderer.dev.ts",
    "dev:main": "webpack --config webpack/webpack.main.dev.ts && cross-env NODE_ENV=development electron .",
    "build:main": "webpack --config webpack/webpack.main.prod.ts",
    "build:renderer": "webpack --env.production --config webpack/webpack.renderer.prod.ts",
    "build": "npm run build:renderer && npm run build:main",
    "electron-pack": "npm run build && electron-builder --config electron-builder.json",
    "electron-pack-dir": "electron-builder --dir --config electron-builder.json",
    "electron-pack-all": "npm run build && electron-builder -mwl --config electron-builder.json",
    "publish": "npm run build && electron-builder -mwl --config electron-builder.json -p always",
    "commit": "cz",
    "prettier": "prettier --config-precedence prefer-file --write .",
    "check": "tsc"
  },
  "keywords": [],
  "author": "luohuidong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luohuidong/electron-music.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@types/classnames": "^2.2.11",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mini-css-extract-plugin": "^1.2.1",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack": "^4.41.24",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.13.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.0",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "core-js": "^3.7.0",
    "electron-log": "^4.3.0",
    "electron-updater": "^4.3.5",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
