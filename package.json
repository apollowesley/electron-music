{
  "name": "electron-react-boilerplate",
  "version": "1.0.1",
  "description": "An electron boilerplate",
  "main": "src/main/index.js",
  "scripts": {
    "start": "concurrently \"npm:webpack-renderer-dev\" \"npm:electron-dev\"",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "webpack-main-build": "webpack --config webpack/webpack.main.prod.js",
    "webpack-renderer-dev": "webpack-dev-server --hot --env.development --config webpack/webpack.renderer.dev.js",
    "webpack-renderer-build": "webpack --env.production --config webpack/webpack.renderer.prod.js",
    "electron-pack": "electron-builder",
    "electron-pack-dir": "electron-builder --dir"
  },
  "build": {
    "appId": "cn.luohuidong.music",
    "files": [
      "src/main/**/*",
      "dist-webpack/renderer/**/*"
    ],
    "mac": {
      "category": "public.app-category.music"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:4000"
      }
    ]
  },
  "keywords": [],
  "author": "luohuidong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luohuidong/electron-react-boilerplate.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "axios": "^0.19.0",
    "core-js": "^3.1.4",
    "electron-log": "^3.0.7",
    "electron-updater": "^4.1.2",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
