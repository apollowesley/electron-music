{
  "name": "snow-music",
  "version": "1.1.0",
  "description": "An electron music app",
  "main": "dist/dist-ts/main/index.js",
  "scripts": {
    "dev:renderer": "webpack-dev-server --hot --env.development --config webpack/webpack.renderer.dev.ts",
    "dev:main": "tsc && cross-env NODE_ENV=development electron .",
    "build:main": "tsc",
    "build:renderer": "webpack --env.production --config webpack/webpack.renderer.prod.ts",
    "build": "npm run build:renderer && npm run build:main",
    "electron-pack": "npm run build && electron-builder -mwl --config electron-builder.json",
    "electron-pack-dir": "electron-builder --dir --config electron-builder.json",
    "publish": "npm run build && electron-builder -mwl --config electron-builder.json -p always",
    "commit": "cz",
    "prettier": "prettier --write ."
  },
  "keywords": [],
  "author": "luohuidong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luohuidong/electron-music.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/classnames": "^2.2.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^10.1.1",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.4.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.11.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "electron-log": "^4.2.2",
    "electron-updater": "^4.3.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
